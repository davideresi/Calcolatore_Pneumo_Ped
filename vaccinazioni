import streamlit as st
from datetime import datetime, timedelta
from fpdf import FPDF
import io

def calcola_eta_mesi(data_nascita):
    """Restituisce l'et√† attuale in mesi"""
    oggi = datetime.today().date()
    return (oggi.year - data_nascita.year) * 12 + (oggi.month - data_nascita.month)

def calcola_eta_alla_dose(data_nascita, data_dose):
    """Restituisce l'et√† in mesi al momento della dose"""
    return (data_dose.year - data_nascita.year) * 12 + (data_dose.month - data_nascita.month)

def calcola_eta(data_nascita):
    oggi = datetime.today()
    anni = oggi.year - data_nascita.year
    mesi = oggi.month - data_nascita.month
    if oggi.day < data_nascita.day:
        mesi -= 1
    eta_mesi = anni * 12 + mesi
    return eta_mesi

def main(data_nascita, eta_mesi, categoria, ha_vaccinazioni, dosi_precedenti):

    st.subheader("üìÖ Calendario raccomandato:")
   

    if ha_vaccinazioni == "No":
        if categoria == "In buona salute":
            if eta_mesi < 7:
                st.success("‚úÖ PCV15 ‚Äì 3 dosi a partire da 2 mesi (con intervalli di 4-8 settimane)")
                eta_target = 2
                oggi = datetime.today()
                data_prox = data_nascita + timedelta(weeks=eta_target*4)
                st.write(f"üìå Prossima data utile per la somministrazione: {data_prox.strftime('%d/%m/%Y')}")
            elif 7 <= eta_mesi < 12:
                st.success("‚úÖ PCV15 ‚Äì 3 dosi a distanza di 8 settimane una dall'altra")
            elif 12 <= eta_mesi < 24:
                st.success("‚úÖ PCV15 ‚Äì 2 dosi a distanza di 8 settimane una dall'altra")
            elif eta_mesi >= 24:
                st.success("‚úÖ PCV20 ‚Äì 1 dose unica")
        elif categoria == "Con patologia a rischio":
            if eta_mesi < 7:
                st.success("‚úÖ PCV20 ‚Äì 4 dosi: 3 dosi a partire da 2 mesi (intervalli 4-8 settimane) + 1 dose a 10-15 mesi")
                st.info("‚ûï 1 dose di PPSV23 raccomandata dopo il compimento dei 24 mesi")
                eta_target = 2
                oggi = datetime.today()
                data_prox = data_nascita + timedelta(weeks=eta_target*4)
                data_prox = max(data_prox, oggi.date())
                st.write(f"üìå Prossima data utile per la somministrazione: {data_prox.strftime('%d/%m/%Y')}")
            elif 7 <= eta_mesi < 12:
                st.success("‚úÖ Nessuna dose precedente")
                st.info("‚ûï Avviare ciclo con 3 dosi di PCV20: 2 a distanza di 4-8 settimane + 1 dose a 10-15 mesi (4-8 settimane dopo la precedente)")
                st.info("‚ûï 1 dose di PPSV23 raccomandata dopo i 24 mesi")
            elif 12 <= eta_mesi < 24:
                st.success("‚úÖ PCV20 ‚Äì 2 dosi a distanza di 8 settimane + PPSV23 dopo i 24 mesi di et√†")
            elif eta_mesi >= 24:
                st.success("‚úÖ PCV20 ‚Äì 1 dose + PPSV23 a distanza di almeno 8 settimane")
        
        
    # --- Bambini con dosi precedenti ---
    if ha_vaccinazioni == "S√¨" and dosi_precedenti:
        for i, dose in enumerate(dosi_precedenti, start=1):
            data = dose["data"]
            vaccino = dose["vaccino"]
            eta_dose = dose["eta_mesi"]

            if vaccino == "PCV13" and data > datetime(2023, 12, 31).date():
                st.warning(f"‚ö†Ô∏è Dose {i}: PCV13 non pi√π in commercio al momento della somministrazione ({data.strftime('%d/%m/%Y')}). Verificare la correttezza dell'inserimento.")

            if vaccino == "PPSV23" and eta_dose < 24:
                st.warning(f"‚ö†Ô∏è Dose {i}: PPSV23 somministrato prima dei 24 mesi. La dose potrebbe essere non valida e va rivalutata.")
    # --- Bambini con eta_mesi < 7 ---
        if categoria == "In buona salute" and eta_mesi < 7:
            n_dosi = len(dosi_precedenti)
            dosi_sorted = sorted(dosi_precedenti, key=lambda x: x['data'])
            tipo_dosi = [d['vaccino'] for d in dosi_sorted]
            eta_dosi = [d['eta_mesi'] for d in dosi_sorted]
            ultima_data = dosi_sorted[-1]['data']

            if all(v == "PCV15" for v in tipo_dosi):
                if n_dosi == 1:
                    st.success("‚úÖ PCV15 ‚Äì 1 dose gi√† eseguita")
                    st.info("Completare con altre 2 dosi: una a partire da 4 mesi e una a 10 mesi.")
                elif n_dosi == 2:
                    st.success("‚úÖ PCV15 ‚Äì 2 dosi gi√† eseguite")
                    st.info("Completare con la 3¬™ dose a 10 mesi (minimo 4 settimane dalla precedente).")
                elif n_dosi >= 3:
                    st.success("‚úÖ PCV15 ‚Äì 3 dosi gi√† eseguite")
                    st.warning("PCV15: ciclo completo. Nessuna ulteriore dose prevista.")

            elif all(v == "PCV20" for v in tipo_dosi):
                if n_dosi == 1:
                    st.success("‚úÖ PCV20 ‚Äì 1 dose gi√† eseguita")
                    st.info("Completare con 3 dosi: 2 a distanza di 4-8 settimane e la 4¬™ tra 10-15 mesi.")
                elif n_dosi == 2:
                    st.success("‚úÖ PCV20 ‚Äì 2 dosi gi√† eseguite")
                    st.info("Completare con 2 dosi: una dopo almeno 4 settimane, l‚Äôaltra tra 10-15 mesi.")
                elif n_dosi == 3:
                    st.success("‚úÖ PCV20 ‚Äì 3 dosi gi√† eseguite")
                    st.info("Completare con la 4¬™ dose tra i 10-15 mesi di et√† (minimo 4 settimane dalla precedente).")
                elif n_dosi >= 4:
                    st.success("‚úÖ PCV20 ‚Äì 4 dosi gi√† eseguite")
                    st.warning("‚ö†Ô∏è Verifica: la 4¬™ dose dovrebbe essere tra i 10-15 mesi di et√†. Se somministrata prima, considerare richiamo.")
            else:
                st.warning("‚ö†Ô∏è Le dosi somministrate non sono omogenee (PCV15/PCV20). Si raccomanda di verificare i dati inseriti.")

        elif categoria == "Con patologia a rischio" and eta_mesi < 7:
            n_dosi = len(dosi_precedenti)
            dosi_sorted = sorted(dosi_precedenti, key=lambda x: x['data'])
            tipo_dosi = [d['vaccino'] for d in dosi_sorted]
            eta_dosi = [d['eta_mesi'] for d in dosi_sorted]
            ultima_data = dosi_sorted[-1]['data']

            
            if n_dosi == 1:
                    st.success("‚úÖ PCV ‚Äì 1 dose gi√† eseguita")
                    st.info("Completare con altre 3 dosi di PCV20: due nei primi mesi, una a 10-15 mesi. Aggiungere PPSV23 dopo i 24 mesi.")
            elif n_dosi == 2:
                    st.success("‚úÖ PCV ‚Äì 2 dosi gi√† eseguite")
                    st.info("Completare con 2 dosi di PCV20: una nei primi mesi, una a 10-15 mesi. PPSV23 dopo 24 mesi.")
            elif n_dosi == 3:
                    st.success("‚úÖ PCV ‚Äì 3 dosi gi√† eseguite")
                    st.info("Completare con 1 dose di PCV20 tra i 10-15 mesi. PPSV23 dopo 24 mesi.")
            elif n_dosi >= 4:
                    st.success("‚úÖ PCV ‚Äì 4 dosi gi√† eseguite")
                    st.warning("‚ö†Ô∏è Verificare et√† al momento delle dosi e considerare PPSV23 dopo 24 mesi.")

            
            else:
                st.warning("‚ö†Ô∏è Dosi miste o anomale (es. PCV15 e PCV20): verificare inserimento. PPSV23 solo dopo i 24 mesi.")

    # --- Bambini con eta_mesi 7-11 ---
        elif categoria == "In buona salute" and 7 <= eta_mesi < 12:
            n_dosi = len(dosi_precedenti)
            dosi_sorted = sorted(dosi_precedenti, key=lambda x: x['data'])
            tipo_dosi = [d['vaccino'] for d in dosi_sorted]
            eta_dosi = [d['eta_mesi'] for d in dosi_sorted]
            ultima_data = dosi_sorted[-1]['data']
           
            if all(v == "PCV15" for v in tipo_dosi):
                if n_dosi == 1:
                    st.success("‚úÖ PCV15 ‚Äì 1 dose gi√† eseguita")
                    st.info("Completare con 2 dosi di PCV15 a distanza di almeno 8 settimane dall'ultima dose eseguita e fra le dosi")
                elif n_dosi == 2:
                        st.success("‚úÖ PCV15 ‚Äì 2 dosi gi√† eseguite")
                        st.warning("Completare con 1 dose di PCV15 a distanza di almeno 8 settimane dall'ultima dose eseguita.")
                elif n_dosi == 3:
                        st.success("‚úÖ PCV15 ‚Äì 3 dosi gi√† eseguite")
                        st.warning("Ciclo considerato completo. Nessuna ulteriore dose prevista.")
                else:
                        st.warning("‚ö†Ô∏è Sono state registrate pi√π di 3 dosi di PCV15. Verificare la correttezza dell'inserimento.")

            elif all(v == "PCV20" for v in tipo_dosi):
                if n_dosi == 1:
                        st.success("‚úÖ PCV20 ‚Äì 1 dose gi√† eseguita")
                        st.info("Completare con 2 dosi: ad una distanza di 4-8 settimane dallp'ultima e fra le dosi.")
                elif n_dosi == 2:
                        st.success("‚úÖ PCV20 ‚Äì 2 dosi gi√† eseguite")
                        st.info("Completare con 1 dose a distanza di 4-8 settimane dall'ultima, tra i 10-15 mesi.")
                elif n_dosi == 3:
                        st.success("‚úÖ PCV20 ‚Äì 3 dosi gi√† eseguite")
                        st.warning("Verificare: la 4¬™ dose va eseguita tra i 10-15 mesi.")
                elif n_dosi >= 4:
                        st.success("‚úÖ PCV20 ‚Äì 4 dosi gi√† eseguite")
                        st.warning("‚ö†Ô∏è Verifica: la 4¬™ dose dovrebbe essere tra i 10-15 mesi di et√†. Se somministrata prima, considerare richiamo.")

                else:
                        st.warning("‚ö†Ô∏è Le dosi somministrate non sono omogenee (PCV15/PCV20). Si raccomanda di verificare i dati inseriti.")

            else:
                st.warning("‚ö†Ô∏è Le dosi somministrate non sono omogenee (PCV15/PCV20). Si raccomanda di verificare i dati inseriti.")

       
        elif categoria == "Con patologia a rischio" and 7 <= eta_mesi < 12:
            n_dosi = len(dosi_precedenti)
            dosi_sorted = sorted(dosi_precedenti, key=lambda x: x['data'])
            tipo_dosi = [d['vaccino'] for d in dosi_sorted]
            eta_dosi = [d['eta_mesi'] for d in dosi_sorted]
            ultima_data = dosi_sorted[-1]['data']

            if n_dosi == 1:
                st.success("‚úÖ 1 dose di PCV gi√† eseguita")
                st.info("‚ûï Completare con 2 dosi: 1 dose a distanza di almeno 4-8 settimane, poi richiamo tra 10-15 mesi")

            elif n_dosi == 2:
                if all(e < 7 for e in eta_dosi):
                    st.success("‚úÖ 2 dosi di PCV gi√† eseguite (entrambe <7 mesi)")
                    st.info("‚ûï Completare con 2 dosi: 1 dose a distanza di almeno 4-8 settimane, poi richiamo tra 10-15 mesi")
                else:
                    st.success("‚úÖ 2 dosi di PCV gi√† eseguite (almeno una ‚â•7 mesi)")
                    st.info("‚ûï Completare con 1 dose tra 10-15 mesi a distanza di almeno 4 settimane")

            elif n_dosi == 3:
                if all(v == "PCV15" for v in tipo_dosi):
                    st.success("‚úÖ 3 dosi di PCV15 gi√† eseguite")
                    st.info("‚ûï Somministrare 1 dose di PCV20 tra i 10-15 mesi, a distanza di almeno 8 settimane dall'ultima dose")
                elif all(v == "PCV20" for v in tipo_dosi):
                    st.success("‚úÖ 3 dosi di PCV20 gi√† eseguite")
                    st.info("‚ûï Ciclo primario completo. Raccomandata 1 dose di PPSV23 dopo i 24 mesi")
                else:
                    st.warning("‚ö†Ô∏è Le dosi somministrate non sono omogenee (PCV15/PCV20). Si raccomanda di verificare i dati inseriti.")
            else:
                    st.warning("‚ö†Ô∏è Le dosi somministrate non sono omogenee (PCV15/PCV20). Si raccomanda di verificare i dati inseriti.")




 # --- Bambini con eta_mesi 12-23 ---  

        elif categoria == "In buona salute" and 12 <= eta_mesi < 24:
            n_dosi = len(dosi_precedenti)
            dosi_sorted = sorted(dosi_precedenti, key=lambda x: x['data'])
            tipo_dosi = [d['vaccino'] for d in dosi_sorted]
            eta_dosi = [d['eta_mesi'] for d in dosi_sorted]
            ultima_data = dosi_sorted[-1]['data']


            if all(v == "PCV15" for v in tipo_dosi):
                if n_dosi == 1:
                    st.success("‚úÖ 1 dose di PCV15 gi√† eseguita")
                    st.info("Completare con 1 dose di PCV15 a distanza di almeno 8 settimane dalla precedente.")
                elif n_dosi == 2:
                    st.success("‚úÖ 2 dosi di PCV15 gi√† eseguite")
                    st.info("Completare con 1 dose di PCV15 a distanza di almeno 8 settimane dalla precedente.")
                elif n_dosi >= 3:
                    st.success("‚úÖ 3 o pi√π dosi di PCV15 gi√† eseguite")
                    st.info("Nessuna ulteriore dose di PCV15 o PCV20 prevista.")

            if all(v == "PCV20" for v in tipo_dosi):
                if n_dosi == 1 and eta_dosi[0] < 12:
                    st.success("‚úÖ PCV20 ‚Äì 1 dose gi√† eseguita (prima dei 12 mesi)")
                    st.info("Completare con 2 dosi a distanza di 8 settimane ciascuna.")
                elif n_dosi == 1 and eta_dosi[0] >= 12:
                    st.success("‚úÖ PCV20 ‚Äì 1 dose gi√† eseguita (dopo i 12 mesi)")
                    st.info("Completare con 1 dose a distanza di almeno 8 settimane.")
                elif n_dosi == 2:
                    if eta_dosi[0] < 12 and eta_dosi[1] < 12:
                        st.success("‚úÖ PCV20 ‚Äì 2 dosi gi√† eseguite (entrambe prima dei 12 mesi)")
                        st.info("Completare con 1 dose a distanza di almeno 8 settimane.")
                    elif eta_dosi[0] >= 12 or eta_dosi[1] >= 12:
                        st.success("‚úÖ PCV20 ‚Äì 2 dosi gi√† eseguite (almeno una dopo i 12 mesi)")
                        st.info("Nessuna ulteriore dose prevista.")
                else:
                    st.warning("‚ö†Ô∏è Vaccini misti o non PCV20. Si raccomanda di verificare i dati inseriti.")
        
        elif categoria == "Con patologia a rischio" and 12 <= eta_mesi < 24:
            n_dosi = len(dosi_precedenti)
            dosi_sorted = sorted(dosi_precedenti, key=lambda x: x['data'])
            tipo_dosi = [d['vaccino'] for d in dosi_sorted]
            eta_dosi = [d['eta_mesi'] for d in dosi_sorted]
            ultima_data = dosi_sorted[-1]['data']

            if n_dosi == 1:
                                  st.success("‚úÖ 1 dose gi√† eseguita")
                                  st.info("Completare con 2 dosi di PCV20 a distanza di almeno 8 settimane.")
                                  st.info("‚ûï PPSV23 raccomandato dopo i 24 mesi.")

            if n_dosi == 2:
                if eta_dosi[0] < 12 or eta_dosi[1] < 12:
                    st.success("‚úÖ 2 dosi gi√† eseguite (almeno una prima dei 12 mesi)")
                    st.info("Completare con 1 dose di PCV20 a distanza di almeno 8 settimane dall'ultima.")
                    st.info("‚ûï PPSV23 raccomandato dopo i 24 mesi.")
                elif all(e >= 12 for e in eta_dosi) and not any(v == "PCV20" for v in tipo_dosi):
                    st.success("‚úÖ 2 dosi gi√† eseguite (entrambe dopo i 12 mesi, ma nessuna con PCV20)")
                    st.info("Completare con 1 dose di PCV20 a distanza di almeno 8 settimane dall'ultima.")
                    st.info("‚ûï PPSV23 raccomandato dopo i 24 mesi.")
                elif all(e >= 12 for e in eta_dosi) and any(v == "PCV20" for v in tipo_dosi):
                    st.success("‚úÖ 2 dosi gi√† eseguite (entrambe dopo i 12 mesi, almeno una con PCV20)")
                    st.info("Nessuna ulteriore dose prevista.")
                    st.info("‚ûï PPSV23 raccomandato dopo i 24 mesi.")

            elif n_dosi == 3:
                if not any(v == "PCV20" for v in tipo_dosi):
                                            st.success("‚úÖ 3 dosi gi√† eseguite (nessuna con PCV20)")
                                            st.info("Completare con 1 dose di PCV20 a distanza di almeno 8 settimane dall'ultima.")
                                            st.info("‚ûï PPSV23 raccomandato dopo i 24 mesi.")
                elif any(v == "PCV20" for v in tipo_dosi):
                                            st.success("‚úÖ 3 dosi gi√† eseguite (almeno una con PCV20)")
                                            st.info("Nessuna ulteriore dose PCV20 prevista.")
                                            st.info("‚ûï PPSV23 raccomandato dopo i 24 mesi.")

            elif n_dosi > 3:
                if any(v == "PCV20" for v in tipo_dosi):
                                            st.success("‚úÖ Pi√π di 3 dosi gi√† eseguite (almeno una con PCV20)")
                                            st.info("Nessuna ulteriore dose PCV20 prevista.")
                                            st.info("‚ûï PPSV23 raccomandato dopo i 24 mesi.")
                else:
                                            st.success("‚úÖ Pi√π di 3 dosi gi√† eseguite (nessuna con PCV20)")
                                            st.info("Completare con 1 dose di PCV20 a distanza di almeno 8 settimane dall'ultima.")
                                            st.info("‚ûï PPSV23 raccomandato dopo i 24 mesi.")

            else:
                    st.warning("‚ö†Ô∏è Vaccini misti o non PCV20. Si raccomanda di verificare i dati inseriti.")

# --- Bambini con eta_mesi 24-59 ---

        elif categoria == "In buona salute" and 24 <= eta_mesi < 60:
            n_dosi = len(dosi_precedenti)
            tipo_dosi = [d["vaccino"] for d in dosi_precedenti]
            eta_dosi = [d["eta_mesi"] for d in dosi_precedenti]

            # üëâ GESTIONE CICLO MISTO: presenza sia di PCV15 che di PCV20
            if "PCV15" in tipo_dosi and "PCV20" in tipo_dosi:
                st.success("‚úÖ Ciclo misto PCV15 + PCV20 rilevato")
                st.info("üí° Si applicano le regole di PCV20")
                eta_pcv20 = [eta_dosi[i] for i, v in enumerate(tipo_dosi) if v == "PCV20"]
                if any(e >= 12 for e in eta_pcv20):
                    st.success("‚úÖ Almeno una dose di PCV20 dopo i 12 mesi")
                    st.info("Nessuna ulteriore dose raccomandata.")
                else:
                    st.warning("‚ö†Ô∏è Tutte le dosi di PCV20 sono state eseguite prima dei 12 mesi")
                    st.info("Somministrare 1 dose di PCV20 a distanza di almeno 8 settimane.")

            # üëâ SOLO PCV15
            elif all(v == "PCV15" for v in tipo_dosi):
                if n_dosi == 1 and eta_dosi[0] >= 24:
                    st.success("‚úÖ 1 dose di PCV15 gi√† eseguita dopo i 24 mesi")
                    st.info("Nessuna ulteriore dose raccomandata.")
                elif n_dosi == 1 and eta_dosi[0] < 24:
                    st.success("‚úÖ 1 dose di PCV15 gi√† eseguita prima dei 24 mesi")
                    st.info("Somministrare 1 dose di PCV20 a distanza di almeno 8 settimane.")
                elif n_dosi == 2:
                    if all(e < 12 for e in eta_dosi):
                        st.success("‚úÖ 2 dosi di PCV15 (entrambe prima dei 12 mesi)")
                        st.info("Somministrare 1 dose di PCV20 a distanza di almeno 8 settimane.")
                    else:
                        st.success("‚úÖ 2 dosi di PCV15 (almeno una dopo i 12 mesi)")
                        st.info("Nessuna ulteriore dose raccomandata.")
                elif n_dosi >= 3:
                    st.success("‚úÖ ‚â•3 dosi di PCV15 gi√† eseguite")
                    st.info("Nessuna ulteriore dose raccomandata.")

            # üëâ SOLO PCV20
            elif "PCV20" in tipo_dosi:
                eta_pcv20 = [eta_dosi[i] for i, v in enumerate(tipo_dosi) if v == "PCV20"]
                if n_dosi == 1:
                    if eta_pcv20[0] >= 12:
                        st.success("‚úÖ 1 dose di PCV20 dopo i 12 mesi")
                        st.info("Nessuna ulteriore dose raccomandata.")
                    else:
                        st.success("‚úÖ 1 dose di PCV20 prima dei 12 mesi")
                        st.info("Somministrare 1 dose di PCV20 a distanza di almeno 8 settimane.")
                elif n_dosi == 2:
                    if any(e >= 12 for e in eta_pcv20):
                        st.success("‚úÖ 2 dosi di PCV20 (almeno una dopo i 12 mesi)")
                        st.info("Nessuna ulteriore dose raccomandata.")
                    else:
                        st.success("‚úÖ 2 dosi di PCV20 (entrambe prima dei 12 mesi)")
                        st.info("Somministrare 1 dose di PCV20 a distanza di almeno 8 settimane.")
                elif n_dosi >= 3:
                    if any(e >= 12 for e in eta_pcv20):
                        st.success("‚úÖ ‚â•3 dosi di PCV20 (almeno una dopo i 12 mesi)")
                        st.info("Nessuna ulteriore dose raccomandata.")
                    else:
                        st.success("‚úÖ ‚â•3 dosi di PCV20 (tutte prima dei 12 mesi)")
                        st.info("Somministrare 1 dose di PCV20 a distanza di almeno 8 settimane.")

            # üëâ PRESENZA DI PCV13
            elif any(v == "PCV13" for v in tipo_dosi):
                st.warning("‚ö†Ô∏è Dose di PCV13 rilevata. Verificare la data di somministrazione.")
                st.info("Somministrare 1 dose di PCV20.")

            # üëâ NESSUNA DOSE PREGRESSA
            else:
                st.info("‚ÑπÔ∏è Nessuna raccomandazione specifica per le condizioni inserite.")



        elif categoria == "Con patologia a rischio" and 24 <= eta_mesi < 60:
            n_dosi = len(dosi_precedenti)
            tipo_dosi = [d["vaccino"] for d in dosi_precedenti]
            eta_dosi = [d["eta_mesi"] for d in dosi_precedenti]

            # Validit√† PPSV23: deve essere somministrato ‚â•24 mesi
            for i, v in enumerate(tipo_dosi):
                if v == "PPSV23" and eta_dosi[i] < 24:
                    st.warning(f"‚ö†Ô∏è La dose {i+1} di PPSV23 √® stata somministrata prima dei 24 mesi ({eta_dosi[i]} mesi). Secondo le raccomandazioni, la dose non √® considerata valida.")
                    st.info("üíâ Ripetere PPSV23 a distanza di almeno 8 settimane e dopo i 24 mesi.")

            ha_ppsv23 = any(v == "PPSV23" and eta_dosi[i] >= 24 for i, v in enumerate(tipo_dosi))
            ha_pcv20 = any(v == "PCV20" for v in tipo_dosi)
            ha_pcv15 = any(v == "PCV15" for v in tipo_dosi)

            # CICLO MISTO PCV15 + PCV20
            if ha_pcv15 and ha_pcv20:
                st.success("‚úÖ Ciclo misto PCV15 + PCV20 rilevato")
                st.info("üí° Si applicano le regole di PCV20 + PPSV23")
                eta_pcv20 = [eta_dosi[i] for i, v in enumerate(tipo_dosi) if v == "PCV20"]

                if any(e >= 24 for e in eta_pcv20):
                    if not ha_ppsv23:
                        st.success("‚úÖ Almeno una dose di PCV20 dopo i 24 mesi")
                        st.info("‚ûï Somministrare PPSV23 a distanza di almeno 8 settimane")
                    else:
                        st.success("‚úÖ PCV20 + PPSV23 gi√† eseguiti")
                        st.info("Nessuna ulteriore dose raccomandata.")
                else:
                    st.warning("‚ö†Ô∏è Tutte le dosi di PCV20 eseguite prima dei 24 mesi")
                    if not ha_ppsv23:
                        st.info("üíâ Somministrare 1 dose di PCV20 + PPSV23 a distanza di almeno 8 settimane")
                    else:
                        eta_ppsv23 = [eta_dosi[i] for i, v in enumerate(tipo_dosi) if v == "PPSV23"]
                        if eta_ppsv23:
                            st.info(f"üíâ Somministrare 1 dose aggiuntiva di PCV20 dopo almeno 12 mesi da PPSV23 ‚Üí dopo i {eta_ppsv23[0] + 12} mesi")
                        else:
                            st.warning("‚ö†Ô∏è PPSV23 rilevato ma impossibile calcolare l'intervallo per la nuova dose di PCV20")
            # SOLO PCV15
            elif all(v == "PCV15" for v in tipo_dosi):
                if n_dosi == 1 and eta_dosi[0] >= 24:
                    st.success("‚úÖ 1 dose di PCV15 eseguita dopo i 24 mesi")
                    st.info("‚ûï Somministrare PPSV23 a distanza di almeno 8 settimane")
                elif n_dosi == 1 and eta_dosi[0] < 24:
                    st.success("‚úÖ 1 dose di PCV15 eseguita prima dei 24 mesi")
                    st.info("Somministrare 1 dose di PCV20 ‚ûï PPSV23 a distanza di almeno 8 settimane")
                elif n_dosi == 2:
                    if all(e < 12 for e in eta_dosi):
                        st.success("‚úÖ 2 dosi di PCV15 (entrambe prima dei 12 mesi)")
                        st.info("Somministrare 1 dose di PCV20 ‚ûï PPSV23 a distanza di almeno 8 settimane")
                    else:
                        st.success("‚úÖ 2 dosi di PCV15 (almeno una dopo i 12 mesi)")
                        st.info("‚ûï Somministrare PPSV23 a distanza di almeno 8 settimane")
                elif n_dosi >= 3:
                    st.success("‚úÖ ‚â•3 dosi di PCV15 gi√† eseguite")
                    if not ha_ppsv23:
                        st.info("‚ûï Somministrare PPSV23 a distanza di almeno 8 settimane")
                    else:
                        st.info("Nessuna ulteriore dose raccomandata.")

            # SOLO PCV20
            elif all(v == "PCV20" for v in tipo_dosi):
                eta_pcv20 = [eta_dosi[i] for i, v in enumerate(tipo_dosi) if v == "PCV20"]
                if any(e >= 24 for e in eta_pcv20):
                    if not ha_ppsv23:
                        st.success("‚úÖ Almeno una dose di PCV20 dopo i 24 mesi")
                        st.info("‚ûï Somministrare PPSV23 a distanza di almeno 8 settimane")
                    else:
                        st.success("‚úÖ PCV20 + PPSV23 gi√† eseguiti")
                        st.info("Nessuna ulteriore dose raccomandata.")
                else:
                    st.warning("‚ö†Ô∏è Tutte le dosi di PCV20 eseguite prima dei 24 mesi")
                    if not ha_ppsv23:
                        st.info("üíâ Somministrare 1 dose di PCV20 + PPSV23 a distanza di almeno 8 settimane")
                    else:
                        eta_ppsv23 = [eta_dosi[i] for i, v in enumerate(tipo_dosi) if v == "PPSV23"]
                        if eta_ppsv23:
                            st.info(f"üíâ Somministrare 1 dose aggiuntiva di PCV20 dopo almeno 12 mesi da PPSV23 ‚Üí dopo i {eta_ppsv23[0] + 12} mesi")
                        else:
                            st.warning("‚ö†Ô∏è PPSV23 rilevato ma impossibile calcolare l'intervallo per la nuova dose di PCV20")

            # PCV15 + PPSV23 ma MANCANTE PCV20
            elif "PCV15" in tipo_dosi and "PPSV23" in tipo_dosi and "PCV20" not in tipo_dosi:
                eta_ppsv23 = [eta_dosi[i] for i, v in enumerate(tipo_dosi) if v == "PPSV23"]
                if eta_ppsv23:
                    st.warning("‚ö†Ô∏è PPSV23 gi√† eseguito, ma manca la dose di PCV20")
                    st.info(f"üíâ Somministrare 1 dose di PCV20 a distanza di almeno 12 mesi da PPSV23 ‚Üí dopo i {eta_ppsv23[0] + 12} mesi di et√†.")
                else:
                    st.warning("‚ö†Ô∏è PPSV23 rilevato, ma impossibile calcolare la distanza temporale")

            # PCV13 + PCV20 + PPSV23 (gestito come PCV20 + PPSV23)
            elif "PCV13" in tipo_dosi and "PCV20" in tipo_dosi and "PPSV23" in tipo_dosi:
                eta_pcv20 = [eta_dosi[i] for i, v in enumerate(tipo_dosi) if v == "PCV20"]
                if any(e >= 24 for e in eta_pcv20):
                    st.success("‚úÖ Ciclo vaccinale PCV20 + PPSV23 rilevato")
                    st.info("‚ÑπÔ∏è La precedente dose di PCV13 non modifica la schedula")
                    st.info("‚úÖ Nessuna ulteriore dose raccomandata.")
                else:
                    eta_ppsv23 = [eta_dosi[i] for i, v in enumerate(tipo_dosi) if v == "PPSV23"]
                    if eta_ppsv23:
                        st.warning("‚ö†Ô∏è PCV20 eseguito prima dei 24 mesi")
                        st.info(f"üíâ Somministrare 1 dose aggiuntiva di PCV20 dopo almeno 12 mesi da PPSV23 ‚Üí dopo i {eta_ppsv23[0] + 12} mesi")
                    else:
                        st.warning("‚ö†Ô∏è PPSV23 rilevato ma impossibile calcolare l'intervallo per la nuova dose di PCV20")

            # PCV13 + PCV20 (nessuna PPSV23)
            elif "PCV13" in tipo_dosi and "PCV20" in tipo_dosi and "PPSV23" not in tipo_dosi:
                eta_pcv20 = [eta_dosi[i] for i, v in enumerate(tipo_dosi) if v == "PCV20"]
                if len(eta_pcv20) >= 2 and any(e >= 12 for e in eta_pcv20):
                    st.success("‚úÖ 2 dosi di PCV20 gi√† eseguite (almeno una dopo i 12 mesi)")
                    st.info("‚ÑπÔ∏è La dose precedente di PCV13 non modifica la schedula")
                    st.info("‚ûï Somministrare PPSV23 a distanza di almeno 8 settimane dall'ultima dose di PCV20")
                else:
                    st.warning("‚ö†Ô∏è Dosi di PCV20 eseguite troppo precocemente")
                    st.info("üíâ Somministrare 1 dose di PCV20 dopo i 12 mesi (se mancante) oppure PPSV23 se il ciclo √® completo")

            # SOLO PPSV23 (nessuna dose PCV)
            elif all(v == "PPSV23" for v in tipo_dosi):
                st.warning("‚ö†Ô∏è PPSV23 gi√† eseguito ma nessuna dose di PCV rilevata")
                st.info("‚ûï Somministrare 1 dose di PCV20 (a distanza di almeno 12 mesi da PPSV23)")

            # CASI NON RICONOSCIUTI
            else:
                st.warning("‚ö†Ô∏è Combinazione di dosi non riconosciuta.")
                st.info("Verificare le date e i tipi di vaccino inseriti.")






     

